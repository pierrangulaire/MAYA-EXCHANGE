# Environment variables for Laravel backend deployment
# Copy this to .env.local when deploying with Laravel backend

# Backend Configuration
VITE_BACKEND_TYPE=laravel
VITE_LARAVEL_API_URL=https://your-domain.com/api

# App Configuration
VITE_APP_NAME=Exchange Pro
VITE_APP_URL=https://your-domain.com

# Laravel-specific settings (these are handled by Laravel backend)
# VITE_SUPABASE_URL=
# VITE_SUPABASE_ANON_KEY=

# Payment Gateway Configuration (handled by Laravel backend)
# These are set in Laravel .env file instead
# MONEROO_API_KEY=your_moneroo_api_key
# NOWPAYMENTS_API_KEY=your_nowpayments_api_key

# Mode
NODE_ENV=production